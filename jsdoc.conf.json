{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": "./src/",
        "includePattern": ".+\\.js$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
//        "plugins/underscore"          // To remove functions that start with _
    ],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "outputSourceFiles": true,
        "linenums": true,
        "includeDate": false,
        "title": "MathLive",
        "pageTitle": "MathLive Docs",
        "description": "Beautifully typeset math made easy",
        "keywords": "latex, tex, math, typesetting, documentation, docs",
        "forkme": "https://github.com/arnog/mathlive"
    },
    "opts": {
        "access": "all",        //  use "public" to ommit the private symbols
        "template": "node_modules/namis",
        "encoding": "utf8",               // same as -e utf8
        "recurse": true,                  // same as -r
        "destination": "./docs",
        "readme": "CONTRIBUTOR_GUIDE.md",
        "sort": false
    }
}
